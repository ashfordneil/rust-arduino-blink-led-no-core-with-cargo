{
  "arch": "avr",
  "data-layout": "e-P1-p:16:8-i8:8-i16:8-i32:8-i64:8-f32:8-f64:8-n8-a:8",
  "env": "",
  "executables": true,
  "_X_REMOVED_is-builtin": true,
  "linker-flavor": "gcc",
  "linker-is-gnu": true,
  "llvm-target": "avr-unknown-unknown",
  "os": "unknown",
  "pre-link-args": {
    "gcc": [
      "-Wl,--as-needed"
    ]
  },
  "target-c-int-width": "16",
  "target-endian": "little",
  "target-pointer-width": "16",
  "vendor": "unknown",

  "COMMENT": "THE FOLLOWING WERE ADDED",

  "exe-suffix": ".elf",
  "cpu": "atmega328p",

  "COMMENT": "This is probably vestigial",
  "no-builtins": true,

  "linker": "avr-gcc",
  "target-env": "gnu",
  "pre-link-args": {
    "gcc": [
      "-mmcu=atmega328p",
      "-nostartfiles",
      "-T",
      "./linker-script",
      "./interrupt_vector.S",
      "./initialize_memory.S"
    ]
  }
}
